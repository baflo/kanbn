{
  "name": "@basementuniverse/kanbn",
  "version": "0.10.0",
  "description": "A CLI Kanban application",
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/esm/main.js",
      "require": "./build/cjs/main.cjs",
      "types": "./src/main.d.ts"
    },
    "./utility": {
      "import": "./build/esm/utility.js",
      "require": "./build/cjs/utility.cjs",
      "types": "./src/utility.d.ts"
    }
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "node build.js",
    "test": "KANBN_ENV=test qunit ./test/**/*.test.js"
  },
  "preferGlobal": true,
  "bin": {
    "kanbn": "bin/kanbn"
  },
  "author": "Gordon Larrigan <gordonlarrigan@gmail.com> (https://gordonlarrigan.com)",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basementuniverse/kanbn.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "kanbn",
    "kanban",
    "task manager",
    "project manager",
    "agile",
    "scrum",
    "sprint"
  ],
  "license": "MIT",
  "dependencies": {
    "asciichart": "^1.5.25",
    "auto-load": "^3.0.4",
    "chrono-node": "^2.1.10",
    "dateformat": "^4.3.1",
    "dotenv": "^8.2.0",
    "front-matter": "^4.0.2",
    "fuzzy": "^0.1.3",
    "git-user-name": "^2.0.0",
    "glob-promise": "^3.4.0",
    "humanize-duration": "^3.25.0",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "inquirer-datepicker": "^2.0.0",
    "inquirer-recursive": "github:basementuniverse/inquirer-recursive",
    "inquirer-select-line": "github:basementuniverse/inquirer-select-line",
    "jsonschema": "^1.4.0",
    "markdown-table": "^3.0.3",
    "marked": "^1.2.7",
    "marked-terminal": "^4.1.1",
    "minimist": "^1.2.5",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rimraf": "^3.0.2",
    "terminal-kit": "^3.1.1",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-select": "^5.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/typescript": "^2.0.0",
    "capture-console": "^1.0.1",
    "esbuild": "^0.24.0",
    "faker": "^5.1.0",
    "mock-argv": "^1.1.10",
    "mock-fs": "^4.13.0",
    "mock-require": "^3.0.3",
    "mock-stdin": "^1.0.0",
    "mockdate": "^3.0.2",
    "qunit": "^2.13.0",
    "strip-ansi": "^6.0.0",
    "typescript": "^4.9.3"
  }
}
